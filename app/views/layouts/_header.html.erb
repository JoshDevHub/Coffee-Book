<header>
  <% if current_user %>
    <%= link_to "Logout",
      destroy_user_session_path,
      data: { turbo_method: :delete } %>
    <%= link_to "Notifications",
      user_notifications_path(current_user)
    %>
    <%= link_to "Friends",
      user_friend_requests_path(current_user)
    %>
  <% end %>
  <%= link_to "Find Friends",
    users_path
  %>
  <% if user_signed_in? %>
    <%= link_to "User Profile",
      user_profile_url(current_user)
    %>
  <% end %>
</header>
